{
  "name": "yago",
  "version": "0.0.1",
  "homepage": "https://github.com/goenning/yago",
  "description": "An asynchronous task runner for Node.js ecosystem.",
  "keywords": [
    "job",
    "background",
    "async",
    "task"
  ],
  "author": "Guilherme Oenning <oenning.ti@gmail.com> (http://goenning.net/)",
  "bugs": {
    "url": "https://github.com/goenning/yago/issues"
  },
  "readmeFilename": "README.md",
  "license": "MIT",
  "repository" : { 
    "type" : "git", 
    "url" : "https://github.com/goenning/yago.git"
  },
  "scripts": {
    "lint": "tslint **/*.ts",
    "build": "tsc",
    "test": "mocha test/",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -- test/ -R spec",
    "check-coverage": "istanbul check-coverage --statements 90 --branches 90 --functions 90 --lines 90",
    "update-coveralls": "cat coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "start-redis": "docker run -d -p 6060:6379 redis"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.14",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.2"
  },
  "dependencies": {
    "cron": "^1.1.0",
    "node-uuid": "^1.4.7",
    "redis": "^2.6.2"
  },
  "engines" : { 
    "node": ">=4" 
  } 
}
